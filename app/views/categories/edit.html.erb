<div class="well" style="width: 36%;margin-top: 2%;margin-left: 29%;">
	<%= form_for @category, :html => {:multipart => true} do |f| %>
	  <p>
	    <%= f.label "Category Name" %><br />
	    <%= f.text_field :name, class: "form-control" %>
	  </p>
	  <br /><br />
	  <p><%= f.submit  class: "btn btn-primary"%></p>
	<% end %>
</div>
<br /><br />